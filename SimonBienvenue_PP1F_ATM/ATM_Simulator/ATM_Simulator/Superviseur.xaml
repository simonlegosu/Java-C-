<Window x:Class="ATM_Simulator.Superviseur"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Superviseur" Height="244" Width="458" Loaded="Window_Loaded">
    <Grid>
        <Button Content="Payer Intérêts Épargne" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="interetEpargne" VerticalAlignment="Top" Width="413" Click="interetEpargne_Click" />
