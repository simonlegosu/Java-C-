        <Button Content="Imprimer Informations Compte" Height="23" HorizontalAlignment="Left" Margin="12,99,0,0" Name="printInfo" VerticalAlignment="Top" Width="413" Click="printInfo_Click" />
        <Button Content="Fermer" Height="23" HorizontalAlignment="Left" Margin="350,172,0,0" Name="button5" VerticalAlignment="Top" Width="75" Click="button5_Click" />
        <Label Content="Argent dans le guichet:" Height="28" HorizontalAlignment="Left" Margin="12,128,0,0" Name="label1" VerticalAlignment="Top" Width="135" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="153,133,0,0" Name="cashGuichet" Text="$$$" VerticalAlignment="Top" Width="160" />
        <Label Content="État du service:" Height="28" HorizontalAlignment="Left" Margin="12,152,0,0" Name="label2" VerticalAlignment="Top" Width="96" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="104,157,0,0" Name="status" Text="" VerticalAlignment="Top" Width="149" />
    </Grid>
</Window>
